{
  /* Click to learn more about project.json  http://go.microsoft.com/fwlink/?LinkID=517074 */
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "dependencies": {
    "EntityFramework.SqlServer": "7.0.0-beta5-12722",
    "EntityFramework.Commands": "7.0.0-beta5-12722",
    "Microsoft.AspNet.Mvc": "6.0.0-beta5-13279",
    /* "Microsoft.AspNet.Mvc.WebApiCompatShim": "6.0.0-beta5-13279", */
    "Microsoft.AspNet.Authentication.Cookies": "1.0.0-beta5-12006",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta5-12604",
    "Microsoft.AspNet.Diagnostics.Entity": "7.0.0-beta5-12604",
    "Microsoft.AspNet.Identity.EntityFramework": "3.0.0-beta5-12035",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta5-11379",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta5-11797",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta5-11602",
    "Microsoft.AspNet.Tooling.Razor": "1.0.0-beta5-13089",
    "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta5-11058",
    "Microsoft.Framework.CodeGenerators.Mvc": "1.0.0-beta5-11933",
    "Microsoft.Framework.Logging": "1.0.0-beta5-11086",
    "Microsoft.Framework.Logging.Console": "1.0.0-beta5-11086",
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-beta3"
  },
  "commands": {
    /* Change the port number when you are self hosting this application */
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000",
    "gen": "Microsoft.Framework.CodeGeneration",
    "ef":  "EntityFramework.Commands"
  },
  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "bundleExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "scripts": {
    "postrestore": [ "npm install", "bower install" ],
    "prepare": [ "gulp copy" ]
  }
}
